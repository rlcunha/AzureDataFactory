{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfLanding"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/pipe_Ingest_full')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "ForEach1",
						"type": "ForEach",
						"dependsOn": [],
						"userProperties": [
							{
								"name": "tables",
								"value": "\"CLientes\""
							}
						],
						"typeProperties": {
							"items": {
								"value": "@variables('tables')",
								"type": "Expression"
							},
							"isSequential": false,
							"batchCount": 1,
							"activities": [
								{
									"name": "Execute Pipeline1",
									"type": "ExecutePipeline",
									"dependsOn": [],
									"userProperties": [],
									"typeProperties": {
										"pipeline": {
											"referenceName": "pipe_Ingest_Generico_excel",
											"type": "PipelineReference"
										},
										"waitOnCompletion": true,
										"parameters": {
											"tbName": {
												"value": "@item()",
												"type": "Expression"
											}
										}
									}
								}
							]
						}
					}
				],
				"variables": {
					"tables": {
						"type": "Array",
						"defaultValue": [
							[
								"Clientes",
								"Clientes"
							],
							[
								"Fornecedores",
								"Fornecedores"
							],
							[
								"Recebimentos",
								"Recebimentos"
							],
							[
								"Produtos",
								"Produtos"
							],
							[
								"Lista Pedidos",
								"Pedidos"
							]
						]
					}
				},
				"annotations": [],
				"lastPublishTime": "2021-09-03T22:17:35Z"
			},
			"dependsOn": []
		}
	]
}