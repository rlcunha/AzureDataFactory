{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfLanding"
		},
		"ls_adls_gestao_cafeteria_servicePrincipalCredential": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalCredential' of 'ls_adls_gestao_cafeteria'"
		},
		"ls_azuresql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_azuresql'"
		},
		"ls_mysql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_mysql'"
		},
		"ls_adls_gestao_cafeteria_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sagestaocafeteria.dfs.core.windows.net"
		},
		"ls_adls_gestao_cafeteria_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "41449c7c-1d3f-4e16-9755-3285e7635e3a"
		},
		"ls_adls_gestao_cafeteria_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "30dbafd1-2a96-4134-b895-6657a38fd138"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
